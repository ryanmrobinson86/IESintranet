ul{
	margin: 11px 0px;
	padding: 0;
	list-style: none;
}
li {
	border-radius: 2px;
	display: block;
	padding: 1px;
	margin: 1px;
}
section ul li.action {
	transition: all 0.2s ease-in-out;
}
section ul li.action:hover {
	cursor: pointer;
	background: rgba(230,230,230,1.0);
	padding: 1px 25px;
	box-shadow: 1px 1px 1px 0 rgba(230,230,230,0.8);
}
section ul li.action.newUser:hover {
	background: rgba(240,180,180,0.7);
}
section#content #headerLinks ul {
	padding-left: 255px;
}
section#content #headerLinks ul li{
	display: inline;
	margin: 45px;
}
li a {
	color: #000;
	text-decoration: none;
}
div#wrapper {
	position: relative;
	top: 0;
	background-color: #fff;
	/*width: 960px;*/
	margin-left: auto;
	margin-right: auto;
	border-radius: 15px;
	box-shadow: 0px 0px 8px 0px rgba(0,0,180,0.8);
}
header#top {
	width: 933px;
	height: 110px;
	margin: 4px;
	padding: 8px;
	position: relative;
	top: 7px;
	border-radius: 15px;
	border: solid 1px;
}
#logo{
	display: inline;
}
#top img{
	display: inline;
	margin: 15px;
	opacity: 1.0;
}
#top img:hover {
	opacity: 0.7;
}
#top nav {
	display: inline;
}
#top nav ul {
	position: absolute;
	top: 40px;
	display: inline;
	list-style: none;
	font: 22px SourceBold, Helvetica;
}
#top nav ul li {
	display: inline;
	margin: 5px;
	padding: 4px;
	border-radius: 4px;
	transition: background 0.4s ease-in-out, box-shadow 0.6s ease-in-out;
}
#top nav ul li:hover {
	background: #ddd;
	box-shadow: 2px 2px 2px 1px #eee;
}
#loginBox{
	font: 14px Source, Helvetica, sans-serif;
	height: 2em;
	border: 1px solid black;
	border-top: none;
	border-right: none;
	width: 42px;
	position: absolute;
	top: 0;
	right: 0;
	background: #eee;
	border-top-right-radius: 15px;
	cursor: default;
}
#loginBoxForm{
	font: 14px Source, Helvetica, sans-serif;
	height: 2em;
	border: 1px solid black;
	border-top: none;
	border-right: none;
	width: 385px;
	position: absolute;
	top: 0;
	right: 0;
	background: #eee;
	border-top-right-radius: 15px;
	visibility: hidden;
	cursor: default;
}
#loginBoxForm fieldset {
	border: none;
	padding: 0.1em 1em;
}
.textField{
	width: 110px;
}
.loginContent{
	margin: 0 1.5em;
	position: absolute;
	top: 0.25em;
	right: 0;
	background: #eee;
}
.loginContent:hover {
	font-family: SourceBold, Helvetica, sans-serif;
}
#loginLogout{
	font-size: 12px;
	opacity: 0.5;
	transition: all 0.4s ease-in-out;
}
#loginLogout:hover {
	opacity: 1;
	text-decoration: underline;
}
#login{
	position: absolute;
	top: 0;
	display: inline-block;
}
