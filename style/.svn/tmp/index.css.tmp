a:hover,
a:focus,
a:active {
	text-shadow: 1px 1px rgba(200,200,200,0.8);
	outline: 1px;
	-webkit-appearance: none;
}
a.noLink {
	text-decoration: none;
	text-shadow: none;
	color: #000;
}
a.noLink:hover,
a.noLink:focus,
a.noLink:active {
	text-decoration: none;
	text-shadow: none;
	color: #000;
	outline: 0px;
	-webkit-appearance: none;
}
a img{
