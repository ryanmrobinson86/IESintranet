#editUser .userTitle {
	padding: 7px;
	margin: 3px;
	width: 125px;
	text-align: right;
	display: inline-block;
	text-decoration: underline;
}
#editUser .userValue {
	display: inline-block;
	padding: 4px;
	margin: 0 0 0 10px;
	min-width: 90px;
	height: 1.5em;
	border: 1px solid #fff;
	transition: all 0.2s ease-in-out;
}
#editUser .userValue:active,
#editUser .userValue:focus,
#editUser .userValue:hover {
	border: 1px solid #ddd;
}
#editUser .userValue.header {
	margin: 0 ;
	min-width: 10px;
	border: 1px solid #fff;
	text-decoration: underline;
}
#editUser .userValue.header:active,
#editUser .userValue.header:focus,
#editUser .userValue.header:hover {
	border: 1px solid #ddd;
}
#editBox {
	font: 14px Source, Helvetica, sans-serif;
	height: 2em;
	border: 1px solid black;
	border-top: none;
	border-right: none;
	width: 162px;
	position: absolute;
	top: 0;
	right: 0;
	background: #eee;
	border-top-right-radius: 15px;
	cursor: default;
}
.editButton{
	margin: 0 1.5em;
	position: relative;
	top: 0.25em;
	right: 0;
	background: #eee;
}
.editButton:hover {
	font-family: SourceBold, Helvetica, sans-serif;
}
.filterSearch {
	display: inline-block;
	padding: 4px;
	margin: 0 0 0 10px;
	min-width: 90px;
	height: 1.5em;
	border: 1px solid #ddd;
	transition: all 0.2s ease-in-out;
}
select.filterSearch{
	height: 2.2em;
}
input.filterSearch:hover,
input.filterSearch:focus,
input.filterSearch:active {
	border: 1px solid #ccc;
	outline: 0px; 
	-webkit-appearance: none;
}
select.filterSearch:focus,
select.filterSearch:active {
	outline: 0px; 
}
.filterButton {
	display: inline-block;
	padding: 4px;
	margin: 8px 0 8px 10px;
	min-width: 60px;
	background: #ddd;
	border: 1px solid #ddd;
	border-radius: 4px;
	transition: all 0.2s ease-in-out;
}
input.filterButton:active,
input.filterButton:focus,
input.filterButton:hover {
	border: 1px solid #ccc;
	background: #eee;
	box-shadow: 1px 1px 3px 0 #eee;
	outline: 0px; 
	-webkit-appearance: none;
}
#projectTable {
	font-size: 12px;
	border: none;
	border-spacing: 0;
	margin-top: 8px;
}
#projectTable th {
	font-size: 13px;
	background: #ddd;
	white-space: nowrap;
}
#projectTable tr:nth-of-type(even) {
	background: #fff;
}
#projectTable tr:nth-of-type(odd) {
	background: #eee;
}
#projectTable td {
	overflow-x: auto;
}
#projectTable .nowrapDiv {
	white-space: nowrap;
}
#projectTable .engineerCell {
	width: 120px;
}
#projectTable .newCell {
	box-shadow: 0 -1px 1px 0 rgba(255,255,200,1) inset;
	background: rgba(255,240,240,0.75);
}
#projectTable.addRowDisp tr:nth-of-type(even) {
	background: #eee;
}
#projectTable.addRowDisp tr:nth-of-type(odd) {
	background: #fff;
}
#projectTable td div,
#projectTable td input,
#projectTable td a,
#projectTable td select {
	display: inline-block;
	margin: 1px;
	padding: 3px;
	margin-left: auto;
	margin-right: auto;
	max-width: 100%;
	border: 1px solid rgba(0,0,0,0.0);
}
#projectTable td input,
#projectTable td select {
	font-size: 11px;
}
#projectTable td textarea {
	font-size: 12px;
	width: 90%;
}
#projectTable td div.projectEditField{
	min-width: 90%;
	min-height: 1.2em;
}
#projectTable td select.projectEditField.newProjectValue,
#projectTable td input.projectEditField.newProjectValue {
	color: black;
}
#projectTable td input.projectEditField.SVN {
	width: 60px;
}
textarea {
	font-family: Source, Helvetica, sans-serif;
}
#projectTable td textarea.projectEditField {
	width: 96%;
	height: 96%;
}
#projectTable .col1{
	width: 100px;
}
#projectTable .col2{
	width: 132px;
}
#projectTable .col3{
	width: 91px;
}
#projectTable .col5{
	width: 264px;
}
#projectTable .col6{
	width: 220px;
}
#projectTable .projectEditField {
	transition: all 0.2s ease-in-out;
}
#projectTable .projectEditField:active,
#projectTable .projectEditField:focus,
#projectTable .projectEditField:hover {
	border: 1px solid #ddd;
}
#projectTable .newProjectValue {
	color: rgba(200,200,200,0.5);
}
#projectTable .newProjectValue:active,
#projectTable .newProjectValue:focus,
#projectTable .newProjectValue:hover {
	color: rgba(100,100,100,0.8);
}
footer {
	min-height: 135px;
	margin: 4px 4px;
	padding: 8px;
	border-radius: 15px;
	border: solid 1px;
	position: relative;
}
footer #links ul li{
	font: 12px/14px Source, sans-serif;
	color: rgba(70,70,70,0.8);
	display: inline;
	list-style-type: none;
	text-indent: -5px;
	margin: 0 3em;
}
footer a{
	color: rgba(70,70,70,0.6);
	text-decoration: none;
	transition: all 0.1s ease-in-out;
}
footer a:hover {
	color: rgba(70,70,70,1.0);
}
footer #links ul li:before {
	content: "-";
	position: relative;
	left: -5px;
}
footer #legal{
	position: absolute;
	bottom: 0;
}
footer h1{
	font: 16px/25px SourceBold, sans-serif;
	color: rgba(80,80,80,0.8);
	margin: 0.4em 0;
}
footer #legal p{
	font: 12px/14px Anadada, serif;
	color: rgba(120,120,120,0.8);
	margin: 0.8em 0;
}
#footerspace {
	width: 100%;
	height: 1px;
}
